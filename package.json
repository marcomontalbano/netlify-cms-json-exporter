{
  "name": "netlify-cms-json-exporter",
  "version": "0.1.0-beta.1",
  "description": "Export all your Netlify CMS contents in a single json file with all relations resolved.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "clean": "npx rimraf node_modules */*/node_modules",
    "lint": "tsc --noEmit",
    "pretest": "yarn lint",
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "netlify cms",
    "netlify",
    "cms",
    "export"
  ],
  "author": "Marco Montalbano <me@marcomontalbano.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcomontalbano/netlify-cms-json-exporter/issues"
  },
  "homepage": "https://github.com/marcomontalbano/netlify-cms-json-exporter#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^12.19.15",
    "jest": "^26.6.3",
    "mock-fs": "^4.13.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/dot-object": "^2.1.2",
    "@types/js-yaml": "^4.0.0",
    "dot-object": "^2.1.4",
    "js-yaml": "^4.0.0"
  }
}
